programa {
  funcao inicio() {
    inteiro num

    escreva("Olá! Seja bem-vindo, por favor digite a sua idade: ")
    leia(num)

    se(num>=18){
      escreva("Maior de idade. Pode continuar o programa.")
    }
    senao{
      escreva("Menor de idade. Não pode continuar o programa.")
    }
    
  }
}
